<!--pages/goodstuff/increase.wxml-->

<view class='container'>
  <view class='thead'>
    <text class='num'>数量</text>
    <text class='time'>时间</text>
  </view>
  <view>
    <view wx:if="{{list==''}}">您没有参加过平台活动，未获得奖励大米！！</view>
    <view wx:for="{{list}}" class='m_10' >
      <text class='num'>{{item.moneyadd}}</text>
      <text class='time'>{{item.addtime}}</text>
    </view>
  </view>
</view>


