/**app.wxss**/
.container {
  height: 100%;
  padding: 0;
  box-sizing: border-box;
} 
image{
  width: 100%;
  height: 100% ;
}
.bg_f5{
  background-color: #f5f5f5
}
.content_center{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
}
.modal {
  position:fixed;
  top:0;
  width:100%;
  height:100%;
  z-index:999;
  background:rgba(0, 0, 0, 0.7);
}
.modal-dialog {
  width:90%;
  margin-top:20%;
  margin-left:5%;
  background:linear-gradient(#ffe744,#ffae00);
  box-sizing:border-box;
  border-radius:10px;
  padding:10px;
}
.modal-title{
  text-align: center;
  font-size: 48rpx
} 
.center{
  text-align: center;
  margin: 5px auto
}
.m_10{
  margin: 10px 0;
}
.m_l_15{
  margin-left:30rpx
}
